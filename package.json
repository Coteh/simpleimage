{
  "name": "simpleimage",
  "version": "1.5.2",
  "private": true,
  "description": "Simple image uploading service",
  "license": "MIT",
  "author": "James Cote (Coteh)",
  "main": "app.js",
  "scripts": {
    "prepare": "husky install",
    "build:client": "webpack",
    "heroku-postbuild": "webpack -p",
    "start": "node app.js",
    "start:debug": "nodemon --inspect=0.0.0.0 .",
    "start:dev": "nodemon",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive --exit $TEST_FILE",
    "test:debug": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive --inspect-brk=0.0.0.0 --exit $TEST_FILE",
    "test:ci": "NODE_ENV=test nyc --reporter=lcovonly mocha test --reporter mocha-junit-reporter --recursive --exit",
    "test:e2e": "cypress run --e2e --browser chrome",
    "test:cypress": "cypress open",
    "format": "prettier --write .",
    "watch:client": "webpack -w"
  },
  "dependencies": {
    "@sentry/browser": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@sentry/webpack-plugin": "^1.18.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "connect-redis": "^3.3.2",
    "copy-webpack-plugin": "^6.4.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.7",
    "ejs-lint": "^1.2.1",
    "exif": "^0.6.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.2",
    "file-type": "^7.4.0",
    "html-entities": "^2.3.3",
    "mime-types": "^2.1.27",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^2.2.9",
    "multer": "^1.4.4",
    "owasp-password-strength-test": "^1.3.0",
    "piexifjs": "1.0.4",
    "rate-limit-redis": "^1.7.0",
    "redis": "^3.1.2",
    "validator": "^13.7.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "winston": "^3.7.2",
    "winston-mongodb": "^5.0.7"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-fetch": "^3.1.5",
    "cypress": "^10.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "mongodb-memory-server": "^8.9.2",
    "mongoist": "^2.5.4",
    "nodemon": "^2.0.16",
    "nyc": "^15.0.1",
    "pixelmatch": "^5.3.0",
    "pngjs": "^6.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "proxyquire": "^2.1.3",
    "session-file-store": "^1.5.0",
    "sinon": "^13.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
