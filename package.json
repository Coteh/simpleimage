{
  "name": "simpleimage",
  "version": "1.5.0",
  "private": true,
  "description": "Simple image uploading service",
  "license": "MIT",
  "author": "James Cote (Coteh)",
  "main": "app.js",
  "scripts": {
    "prepare": "husky install",
    "build:client": "webpack",
    "heroku-postbuild": "webpack -p",
    "start": "node app.js",
    "start:debug": "nodemon --inspect=0.0.0.0 .",
    "start:dev": "nodemon",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive --exit $TEST_FILE",
    "test:debug": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive --inspect-brk=0.0.0.0 --exit $TEST_FILE",
    "test-ci": "NODE_ENV=test nyc --reporter=lcovonly mocha test --reporter mocha-junit-reporter --recursive --exit",
    "test-ui": "cypress",
    "format": "prettier --write .",
    "watch:client": "webpack -w"
  },
  "dependencies": {
    "@sentry/browser": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "@sentry/webpack-plugin": "^1.18.8",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "connect-redis": "^3.3.2",
    "copy-webpack-plugin": "^6.4.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "ejs": "^2.5.7",
    "ejs-lint": "^1.2.1",
    "exif": "^0.6.0",
    "express": "^4.14.0",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "file-type": "^7.4.0",
    "html-entities": "^2.3.2",
    "mime-types": "^2.1.27",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^2.2.9",
    "multer": "^1.4.4",
    "owasp-password-strength-test": "^1.3.0",
    "piexifjs": "^1.0.4",
    "rate-limit-redis": "^1.7.0",
    "redis": "^3.1.2",
    "validator": "^13.7.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.10",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-fetch": "^3.1.5",
    "cypress": "^9.1.1",
    "cypress-file-upload": "^5.0.8",
    "husky": "^7.0.4",
    "mocha": "^7.1.2",
    "mocha-junit-reporter": "^2.0.2",
    "mongodb-memory-server": "^6.7.0",
    "mongoist": "^2.5.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.0.1",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "proxyquire": "^2.1.3",
    "session-file-store": "^1.2.0",
    "sinon": "^9.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
